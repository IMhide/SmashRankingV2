<div class='row justify-content-center'>
  <div class="col col-7">
    <div class='row py-3'>
      <div class="col">
        <h1> 
          <%= @ranking.name %>
        </h1> 
      </div>
      <div class="col">
        <%= render RankSearchComponent.new %>
      </div>
    </div>
    <%= render RankHeaderComponent.new %>
    <%= turbo_frame_tag 'rankings' do %>
      <%= render RankListComponent.new(ranking: @ranking, query: @query) %>
    <% end %>
  </div>
</div>
